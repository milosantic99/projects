type Query{
    users: [User]
    userById(id: ID!): User
}

type Mutation{
    addUser(user: UserAddDto!): User
    updateUser(user: UserUpdateDto!): User
    deleteUser(id: ID!): User
}

input UserAddDto{
    name: String!
    email: String!
}

input UserUpdateDto{
    id: ID!
    name: String!
    email: String!
}

type User{
    id: ID!
    name: String!
    email: String!
}